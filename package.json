{
  "name": "@iwpnd/rip-ts",
  "version": "1.1.7",
  "description": "rest-client",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/iwpnd/rip-ts.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "B. Ramser <iwpnd@posteo.de>",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "rest-client"
  ],
  "engines": {
    "node": ">=20.x",
    "yarn": "^1.22.x"
  },
  "private": false,
  "scripts": {
    "postinstall": "yarn husky init",
    "prepublishOnly": "pinst --disable && yarn run build",
    "postpublish": "pinst --enable",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf {dist,tsconfig.tsbuildinfo,yarn-error.log,coverage}",
    "lint": "eslint ./src",
    "test": "jest --runInBand --verbose --testMatch '**/*test.ts' --coverage",
    "format": "prettier --write src/{*.ts,**/*.ts}",
    "u": "yarn upgrade-interactive --latest",
    "coverage": "FILE=./coverage/lcov-report/index.html; test -f $FILE && open $FILE || echo 'no coverage yet, run yarn test'",
    "prepare": "husky"
  },
  "dependencies": {
    "undici": "6.19.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/core": "0.6.0",
    "@eslint/js": "9.11.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.3.5",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@types/eslint__js": "8.42.3",
    "@types/estree": "1.0.6",
    "@types/jest": "29.5.13",
    "@types/json-schema": "7.0.15",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "devmoji": "2.3.0",
    "eslint": "9.13.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "semantic-release": "24.1.1",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2",
    "typescript-eslint": "8.6.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
